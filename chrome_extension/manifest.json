{
  "manifest_version": 2,

  "name": "youtube-dl",
  "description": "Download videos from different sites.",
  "version": "0.1.1",
  
  "homepage_url": "https://github.com/jaimeMF/youtube-dl_extensions",
  
  "update_url": "http://jaimemf.github.com/youtube-dl_extensions/updates.xml",
  
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "browser_action": {
    "default_icon": "icon_button.png",
    "default_popup": "popup.html"
  },
  
  "options_page": "options.html",
  
   "permissions": [
    "tabs"
  ]
}
